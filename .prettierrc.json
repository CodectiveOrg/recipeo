{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^(vite|@vitejs)(/(.*))?$",
    "^(react|react-dom)(/(.*))?$",
    "^react-router(/(.*))?$",
    "^react-error-boundary(/(.*))?$",
    "^@tanstack(/(.*))?$",
    "^swiper(/(.*))?$",
    "^@iconify(/(.*))?$",
    "^@iconify-json(/(.*))?$",
    "^react-toastify(/(.*))?$",
    "^clsx(/(.*))?$",
    "<THIRD_PARTY_MODULES>",
    "^@/api",
    "^@/components",
    "^@/dto",
    "^@/entities",
    "^@/guards",
    "^@/hooks",
    "^@/layouts",
    "^@/pages",
    "^@/providers",
    "^@/queries",
    "^@/stores",
    "^@/styles",
    "^@/types",
    "^@/utils",
    "^(\\.|\\.\\.)/(.(?!.(css|scss)))*$",
    "\\.(css|scss)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderGroupNamespaceSpecifiers": true
}
